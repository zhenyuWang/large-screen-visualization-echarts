{
  "name": "large-screen-visualization-echarts",
  "version": "0.0.1",
  "description": "A Vue3 based out of the box large screen visual development framework",
  "keywords": [
    "koa",
    "template",
    "cli"
  ],
  "license": "MIT",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:uat": "vue-tsc --noEmit && vite build  --mode uat",
    "preview": "vite preview",
    "lint": "eslint --ext .js,ts,vue src/** --no-error-on-unmatched-pattern",
    "lint:fix": "eslint --ext .js,ts,vue src/** --no-error-on-unmatched-pattern --fix",
    "test": "jest"
  },
  "gitHooks": {
    "commit-msg": "node scripts/verify-commit-msg.js",
    "pre-commit": "lint-staged",
    "pre-push": "npm run test"
  },
  "lint-staged": {
    "*.{js,ts,vue}": "eslint --fix",
    "*": "prettier -w -u"
  },
  "dependencies": {
    "@typescript-eslint/experimental-utils": "^4.33.0",
    "axios": "^0.24.0",
    "echarts": "^5.2.2",
    "screenfull": "^6.0.1",
    "vue": "^3.2.23",
    "vue-awesome-swiper": "^4.1.1",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.9",
    "@babel/preset-typescript": "^7.14.5",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.10",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "@vitejs/plugin-vue": "^1.10.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "^2.0.0-rc.11",
    "autoprefixer": "^10.2.5",
    "babel-jest": "26.6.3",
    "cross-env": "^7.0.3",
    "cypress": "^8.2.0",
    "eslint": "^7.20.0",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.6.0",
    "jest": "26.6.3",
    "lint-staged": "^11.1.2",
    "postcss": "^8.4.4",
    "prettier": "^2.2.1",
    "sass": "^1.32.8",
    "ts-jest": "^26.5.6",
    "typescript": "^4.4.4",
    "vite": "^2.7.0",
    "vite-plugin-externals": "^0.4.0",
    "vite-plugin-imagemin": "^0.6.1",
    "vite-plugin-mock": "^2.9.4",
    "vue-jest": "^5.0.0-alpha.10",
    "vue-tsc": "^0.28.10",
    "yorkie": "^2.0.0"
  }
}
